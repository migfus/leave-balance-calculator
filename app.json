{
  "expo": {
    "name": "Leave Credit Balance Calculator",
    "slug": "leave-credit-balance-calculator",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/android/play_store_512.png",
    "scheme": "leavecreditbalancecalculator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android/play_store_512.png",
        "backgroundImage": "./assets/images/android/play_store_512.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.migfus.leavecreditbalancecalculator"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/web/favicon.ico",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/web/icon-512-maskable.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "20b218e3-5408-485f-88cf-b6d1ccb7b489"
      }
    }
  }
}
